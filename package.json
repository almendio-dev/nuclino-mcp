{
  "name": "@blubblub/mcp-nuclino",
  "version": "1.0.0",
  "description": "MCP server for Nuclino integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "start": "TRANSPORT_TYPE=http node dist/index.js",
    "start:stdio": "TRANSPORT_TYPE=stdio node dist/index.js",
    "dev": "TRANSPORT_TYPE=http tsx src/index.ts",
    "dev:stdio": "TRANSPORT_TYPE=stdio tsx src/index.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "nuclino",
    "http",
    "sse",
    "server-sent-events"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "typescript": "^5.8.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6"
  },
  "bin": {
    "mcp-nuclino": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
