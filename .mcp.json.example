{
	"mcpServers": {
		"nuclino": {
			"_description": "Nuclino MCP server - SSE transport for HTTP integration. NUCLINO_API_KEY must be set in the server's .env file. Optional: Include X-MCP-Auth-Key header if server has MCP_AUTH_KEY configured.",
			"url": "http://localhost:3000/mcp",
			"type": "sse",
			"headers": {
				"X-MCP-Auth-Key": "your-mcp-auth-key-here (only if MCP_AUTH_KEY is set in server .env)"
            }
        },
        "nuclino-stdio": {
			"_description": "Nuclino MCP server - stdio transport for local integration",
			"command": "/path/to/your/nuclino-mcp/run-stdio.sh",
			"env": {
				"NUCLINO_API_KEY": "your-api-key-here"
			}
		}
	}
}
